{
  "name": "inceptjs",
  "version": "0.0.9",
  "description": "A pluggable React framework",
  "author": "Chris <chris@incept.asia>",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "incept": "./bin/incept.js"
  },
  "scripts": {
    "test": "mocha",
    "build": "tsc"
  },
  "files": [
    "bin",
    "dist"
  ],
  "purposes": {
    "cookie": "Cookie Parser",
    "mime": "Get mime type from file name",
    "react-dom": "Convert react to string",
    "react-is": "Tests if functions are react components",
    "@babel/preset-env": "Converts ES6 back to regular JS",
    "@babel/preset-react": "Converts JSX files to regular JS",
    "@babel/register": "Hijacks the require() in order to convert ES6 to regular JS",
    "babel-loader": "Allows transpiling JS files using Babel and webpack",
    "babel-plugin-react-require": "Adds import React from react in all react files",
    "webpack-node-externals": "ignores node_modules when webpack compiles the server (mainly for server side packing)",
    "@pmmmwh/react-refresh-webpack-plugin": "For react refresh (dev mode)",
    "webpack-dev-middleware": "For react refresh (dev mode)",
    "webpack-hot-middleware": "For react refresh (dev mode)",
    "webpack-virtual-modules": "Used to inject hydrate",
    "babel-plugin-module-resolver": "module alias"
  },
