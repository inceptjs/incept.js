{
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev": "cd examples/with-next && npm run dev",
    "build": "npm run build:generator && npm run build:incept && npm run build:react && npm run build:tailwind && npm run build:terminal && npm run build:translate && npm run build:types",
