    "build:tailwind": "cd packages/tailwind && npm run build",
    "build:terminal": "cd packages/terminal && npm run build",
    "build:translate": "cd packages/translate && npm run build",
    "build:types": "cd packages/types && npm run build"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}